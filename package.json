{
  "name": "shrinkit",
  "version": "1.0.0",
  "description": "drag and drop image compressor",
  "main": "build/main.js",
  "repository": "https://github.com/Lakston/shrinkit.git",
  "author": "Fabrice_Pomata <fab.pomata@gmail.com>",
  "license": "MIT",
  "build": {
    "appId": "com.electron.shrinkit",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/mozjpeg": "^5.0.0",
    "@types/pngquant-bin": "^4.0.0",
    "@types/svgo": "^1.0.1",
    "electron": "^3.0.3",
    "electron-builder": "^20.28.4",
    "electron-webpack": "^2.3.1",
    "electron-webpack-ts": "^2.1.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "spectron": "^4.0.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3",
    "webpack": "^4.20.2"
  },
  "dependencies": {
    "mozjpeg": "^6.0.0",
    "pngquant-bin": "^5.0.0",
    "source-map-support": "^0.5.9",
    "svgo": "^1.1.1"
  }
}
